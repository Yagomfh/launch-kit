{
  "name": "launchkit-cli",
  "version": "1.2.0",
  "description": "CLI to generate boilerplate code to quickly launch your first product",
  "repository": "https://github.com/Yagomfh/launch-kit",
  "homepage": "https://github.com/Yagomfh/launch-kit#readme",
  "main": "index.js",
  "author": "Yago Mfh <yagomfh@gmail.com>",
  "license": "MIT",
  "bin": {
    "launchkit-cli": "./lib/bin.cjs"
  },
  "type": "module",
  "keywords": [
    "cli",
    "npmtool",
    "launchkit",
    "boilerplate"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "cli": "node ./lib/bin.cjs",
    "copy-assets": "copyfiles -r _templates/**/* ./lib && cp package.json ./lib",
    "release": "node ./scripts/release.js"
  },
  "files": [
    "lib/",
    "README.md"
  ],
  "dependencies": {
    "commander": "^12.0.0",
    "copyfiles": "^2.4.1",
    "enquirer": "^2.4.1",
    "fs-extra": "^11.2.0",
    "hygen": "^6.1.0",
    "inquirer": "^9.2.17",
    "plop": "^4.0.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.4"
  },
  "devDependencies": {
    "@swc/core": "^1.4.12",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "tsup": "^8.0.2"
  }
}
